{
  "name": "app-wishxner",
  "description": "Wish server cloner is an app that will generally help you completely with the cloning of your favourite server.",
  "version": "1.0.0",
  "exports": null,
  "type": "module",
  "scripts": {
    "start": "tsx src/.",
    "hooks:init": "husky",
    "hooks:lint": "lint-staged",
    "ci:release": "semantic-release",
    "ci:commitlint": "commitlint --from=HEAD~10",
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k4itrun/app-wishxner.git"
  },
  "keywords": [],
  "author": {
    "name": "k4itrun",
    "email": "billoneta@proto.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k4itrun/app-wishxner/issues"
  },
  "homepage": "https://github.com/k4itrun/app-wishxner#readme",
  "dependencies": {
    "chalk": "^5.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@dotenvx/dotenvx": "^1.41.0",
    "@k4i/config": "^1.0.1",
    "@types/node": "^22.14.1",
    "eslint": "^9.25.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier . --write --cache --ignore-unknown"
    ],
    "*.{ts,tsx,js}": [
      "eslint . --fix"
    ]
  },
  "packageManager": "pnpm@10.8.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}